			$(document).ready(function(){
				$(".tip").tipsy({fade: true});
				$('.badge_hint').tipsy({gravity: 's', fade: true});
				$('.disabled').tipsy({gravity: 'n', fade: true});

				refreshTime(".timezoner", "D MMM YY", true);
				
				$(".tab").click(function(){
					$(".tab-area li").removeClass('active');
					$(this).parent().addClass('active');
					$(".list").hide();
					$($(this).attr("href")).show();
					return false;
				});
			});

